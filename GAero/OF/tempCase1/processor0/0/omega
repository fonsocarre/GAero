FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.254748;

boundaryField
{
    PROFILE
    {
        type            omegaWallFunction;
        value           uniform 0.254748;
    }
    INLET
    {
        type            inletOutlet;
        inletValue      uniform 0.254748;
        value           uniform 0.254748;
    }
    OUTLET
    {
        type            inletOutlet;
        inletValue      uniform 0.254748;
        value           uniform 0.254748;
    }
    EMPTY0
    {
        type            empty;
    }
    EMPTY1
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0.254748;
    }
}


